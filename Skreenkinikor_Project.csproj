  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\user-white.png" />
    <Resource Include="Images\key-white.png" />
    <Resource Include="Images\LogInBG.png" />
    <Resource Include="Images\SK_Logo.png" />
  </ItemGroup>

</Project>
