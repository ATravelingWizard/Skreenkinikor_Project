    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Sterkinikor.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\key-white.png" />
    <None Remove="Images\LogInBG.png" />
    <None Remove="Images\SK_Logo.png" />
    <None Remove="Images\user-white.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Sterkinikor.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EntityFramework" Version="6.4.4" />
    <PackageReference Include="FontAwesome.Sharp" Version="6.3.0" />
